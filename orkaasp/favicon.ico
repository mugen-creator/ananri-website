<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Favicon Generator</title>
</head>
<body>
    <canvas id="canvas" width="32" height="32"></canvas>
    <script>
        // Create favicon programmatically
        const canvas = document.getElementById('canvas');
        const ctx = canvas.getContext('2d');
        
        // Create gradient
        const gradient = ctx.createLinearGradient(0, 0, 32, 32);
        gradient.addColorStop(0, '#667eea');
        gradient.addColorStop(1, '#764ba2');
        
        // Draw rounded rectangle
        ctx.fillStyle = gradient;
        ctx.beginPath();
        ctx.moveTo(6, 0);
        ctx.lineTo(26, 0);
        ctx.quadraticCurveTo(32, 0, 32, 6);
        ctx.lineTo(32, 26);
        ctx.quadraticCurveTo(32, 32, 26, 32);
        ctx.lineTo(6, 32);
        ctx.quadraticCurveTo(0, 32, 0, 26);
        ctx.lineTo(0, 6);
        ctx.quadraticCurveTo(0, 0, 6, 0);
        ctx.fill();
        
        // Draw diamond
        ctx.fillStyle = 'white';
        ctx.beginPath();
        ctx.moveTo(8, 16);
        ctx.lineTo(16, 8);
        ctx.lineTo(24, 16);
        ctx.lineTo(16, 24);
        ctx.closePath();
        ctx.fill();
        
        // Convert to ICO format would require server-side processing
        // This is a placeholder that browsers will understand
    </script>
</body>
</html>